{
  "name": "educodar",
  "version": "1.0.0",
  "description": "Educodar Gatsby website",
  "main": "n/a",
  "dependencies": {
    "@material-ui/styles": "4.9.0",
    "@mdx-js/mdx": "1.5.7",
    "@mdx-js/react": "1.5.7",
    "gatsby": "2.19.43",
    "gatsby-image": "2.2.43",
    "gatsby-plugin-csp": "1.1.3",
    "gatsby-plugin-html-attributes": "1.0.5",
    "gatsby-plugin-layout": "1.1.23",
    "gatsby-plugin-manifest": "2.2.47",
    "gatsby-plugin-material-ui": "2.1.6",
    "gatsby-plugin-mdx": "1.0.83",
    "gatsby-plugin-netlify": "2.1.34",
    "gatsby-plugin-netlify-cms": "4.1.41",
    "gatsby-plugin-netlify-identity-widget": "1.1.1",
    "gatsby-plugin-prefetch-google-fonts": "1.4.3",
    "gatsby-plugin-react-helmet": "3.1.23",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sitemap": "2.2.29",
    "gatsby-plugin-typography": "2.3.24",
    "logrocket": "1.0.6",
    "moment": "2.24.0",
    "muy": "0.5.6",
    "netlify-cms-app": "2.11.32",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@material-ui/core": "4.9.5",
    "@testing-library/cypress": "6.0.0",
    "babel-jest": "25.1.0",
    "codecov": "3.6.5",
    "cypress": "4.2.0",
    "eslint-plugin-cypress": "2.10.3",
    "gatsby-cli": "2.10.10",
    "husky": "4.2.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "netlify": "3.1.0",
    "prettier": "1.19.1",
    "react-test-renderer": "16.13.0",
    "start-server-and-test": "1.10.10"
  },
  "scripts": {
    "build": "gatsby build",
    "ci": "npm run testc && npm run report-coverage",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "deploy": "git push",
    "develop": "gatsby develop -H 0.0.0.0 --port 8000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "open:local": "open http://localhost:8000",
    "open:prod": "open https://educodar.com.br",
    "postcheckout": "npm run clean",
    "precommit": "npm run test:staged",
    "prepush": "npm test && npm run test:e2e:ci && npm run build",
    "report-coverage": "codecov",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "testc": "jest --coverage",
    "testu": "jest -u",
    "testw": "jest --watch",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "test:staged": "sh bin/test-staged.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/educodar/website.git"
  },
  "keywords": [
    "educodar",
    "gatsby",
    "react"
  ],
  "author": "ThoughtWorks",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/educodar/website/issues"
  },
  "homepage": "https://github.com/educodar/website#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "npm run postcheckout",
      "pre-commit": "npm run precommit",
      "pre-push": "npm run precommit"
    }
  }
}
